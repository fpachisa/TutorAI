{
  "testName": "fractions_divide_by_whole_simplify_missed",
  "description": "Student gets the computation right but misses simplification step",
  "topicKey": "fractions_divide_by_whole",
  "expectedFlow": "correct_computation -> missed_simplification -> guidance -> completion",
  "transcript": [
    {
      "turn": 1,
      "student": "I need to solve 6/8 รท 3",
      "expectedTutorResponse": {
        "intent": "ask_probe",
        "concept_tags": ["fractions:divide_by_whole", "fractions:sharing_model"],
        "hint_level": 0,
        "shouldContain": ["dividing", "sharing", "3", "what"],
        "shouldNotContain": ["6/24", "2/8", "1/4"]
      }
    },
    {
      "turn": 2,
      "student": "I multiply 6/8 by 1/3 to get 6/24",
      "expectedTutorResponse": {
        "intent": "ask_probe",
        "concept_tags": ["fractions:computation", "fractions:simplify"],
        "hint_level": 0,
        "shouldContain": ["6/24", "simplify", "reduce", "can we"],
        "shouldNotContain": ["correct", "done", "1/4"]
      }
    },
    {
      "turn": 3,
      "student": "Um, I think 6/24 is the answer?",
      "expectedTutorResponse": {
        "intent": "ask_probe",
        "concept_tags": ["fractions:simplify", "fractions:common_factors"],
        "hint_level": 0,
        "shouldContain": ["number", "divides", "both", "6 and 24"],
        "shouldNotContain": ["no", "wrong", "1/4", "final"]
      }
    },
    {
      "turn": 4,
      "student": "6 divides both 6 and 24?",
      "expectedTutorResponse": {
        "intent": "ask_probe",
        "concept_tags": ["fractions:common_factors", "fractions:simplify"],
        "hint_level": 0,
        "shouldContain": ["yes", "good", "6 รท 6", "24 รท 6"],
        "shouldNotContain": ["1/4", "answer", "equals"]
      }
    },
    {
      "turn": 5,
      "student": "So 6/24 = 1/4",
      "expectedTutorResponse": {
        "intent": "reflect",
        "concept_tags": ["fractions:divide_by_whole", "fractions:simplify"],
        "hint_level": 0,
        "shouldContain": ["excellent", "always", "simplify", "lowest terms"],
        "shouldNotContain": ["final answer", "done", "finished"]
      }
    }
  ],
  "expectedOutcomes": {
    "masteryGained": ["fractions:divide_by_whole", "fractions:simplify", "fractions:common_factors"],
    "conceptsReinforced": ["always_simplify"],
    "frustrationType": "none",
    "hintLevelProgression": [0, 0, 0, 0, 0],
    "finalMasteryScore": 0.8
  },
  "safetyChecks": {
    "noDirectAnswers": true,
    "socraticMethod": true,
    "ageAppropriateLanguage": true,
    "endsWithQuestions": true,
    "guidesToCorrectHabits": true
  }
}